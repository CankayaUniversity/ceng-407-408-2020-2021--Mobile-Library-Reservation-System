<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:accessibilityHeading="true"
    android:background="#C2C2C2"
    android:backgroundTint="@color/mlrs_mlrs_mainpageBlue"
    android:scrollbarThumbHorizontal="@drawable/ic_launcher_foreground"
    tools:context=".Activity_LM_Seelect_Date_and_Time_Slot">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="104dp"
        android:layout_marginEnd="0dp"

        android:layout_marginBottom="-10dp"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_search_table_next"
            style="?android:textAppearanceSmall"
            android:layout_width="210dp"
            android:layout_height="55dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="83dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="83dp"
            android:layout_marginBottom="224dp"
            android:backgroundTint="@color/mlrs_white_button"
            android:text="@string/search_table"
            android:textColor="@color/Black"
            android:textSize="18sp"
            android:textStyle="bold" />

        <RelativeLayout
            android:layout_width="362dp"
            android:layout_height="410dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="22dp"
            android:layout_marginTop="3dp"
            android:layout_marginEnd="22dp"
            android:layout_marginBottom="305dp"
            android:background="@drawable/sekil"
            android:orientation="vertical">


            <EditText
                android:id="@+id/et_time_to"
                android:layout_width="135dp"
                android:layout_height="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="310dp"
                android:layout_marginEnd="40dp"
                android:autofillHints=""
                android:background="#C2C2C2"
                android:ems="10"
                android:inputType="time" />

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="34dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="159dp"
                android:layout_marginBottom="95dp"
                android:fontFamily="sans-serif-black"
                android:text="@string/to"
                android:textColor="#040303"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView15"
                android:layout_width="61dp"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="266dp"
                android:layout_marginBottom="106dp"
                android:fontFamily="sans-serif-black"
                android:text="@string/from"
                android:textColor="#040303"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="34dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="239dp"
                android:layout_marginBottom="142dp"
                android:fontFamily="sans-serif-black"
                android:text="@string/time_slot"
                android:textColor="#040303"
                android:textSize="18sp" />

            <RelativeLayout
                android:layout_width="300dp"
                android:layout_height="123dp"
                android:layout_marginStart="45dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="44dp"
                android:layout_marginBottom="5dp"
                android:background="@color/mlrs_lightBlue"
                android:orientation="vertical">

                <TextView
                    android:id="@+id/textView11"
                    android:layout_width="260dp"
                    android:layout_height="30dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="15dp"
                    android:layout_marginBottom="30dp"
                    android:ems="40"
                    android:text="@string/please_select_date_and_time_slot"
                    android:textAlignment="center"
                    android:textColor="@color/Black"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView12"
                    android:layout_width="260dp"
                    android:layout_height="30dp"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="6dp"
                    android:ems="30"
                    android:fontFamily="sans-serif"
                    android:paddingStart="50dp"
                    android:text="@string/for_your_reservation"
                    android:textColor="@color/Black"
                    android:textSize="15sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView17"
                    android:layout_width="match_parent"
                    android:layout_height="39dp"
                    android:fontFamily="sans-serif-condensed"
                    android:paddingStart="15dp"
                    android:text="@string/cebeci_public_library"
                    android:textColor="#FFFFFF"
                    android:textSize="24sp"
                    android:textStyle="bold" />


            </RelativeLayout>

            <TextView
                android:id="@+id/textView13"
                android:layout_width="62dp"
                android:layout_height="34dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="259dp"
                android:layout_marginBottom="225dp"
                android:fontFamily="sans-serif-black"
                android:text="@string/date"
                android:textColor="#040303"
                android:textSize="18sp" />

            <EditText
                android:id="@+id/et_date"
                android:layout_width="wrap_content"
                android:layout_height="36dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="116dp"
                android:layout_marginBottom="194dp"
                android:autofillHints=""
                android:background="#C2C2C2"
                android:ems="10"
                android:inputType="date" />

            <EditText
                android:id="@+id/et_time_from"
                android:layout_width="135dp"
                android:layout_height="36dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="310dp"
                android:layout_marginEnd="191dp"
                android:autofillHints=""
                android:background="#C2C2C2"
                android:ems="10"
                android:inputType="time" />

            <TextView
                android:id="@+id/textView18"
                android:layout_width="280dp"
                android:layout_height="2dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentBottom="true"
                android:layout_marginEnd="44dp"
                android:layout_marginBottom="265dp"
                android:background="@color/Black" />


        </RelativeLayout>

        <Button
            android:id="@+id/btn_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="24dp"
            android:layout_marginBottom="31dp"
            android:background="@color/mlrs_white_button"
            android:backgroundTint="@color/colorPrimaryDark"
            android:text="Back"
            android:textColor="@color/mlrs_white_button"
            android:textColorHighlight="@color/mlrs_white_button"
            android:textColorHint="@color/blue1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />


        <Button
            android:id="@+id/btn_next"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="31dp"
            android:background="@color/mlrs_white_button"
            android:backgroundTint="@color/colorPrimaryDark"
            android:text="Next"
            android:textColor="@color/mlrs_white_button"
            android:textColorHighlight="@color/mlrs_white_button"
            android:textColorHint="@color/blue1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

        <Button
            android:id="@+id/btn_next2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginEnd="24dp"
            android:layout_marginBottom="31dp"
            android:background="@color/mlrs_white_button"
            android:backgroundTint="@color/colorPrimaryDark"
            android:text="Next"
            android:textColor="@color/mlrs_white_button"
            android:textColorHighlight="@color/mlrs_white_button"
            android:textColorHint="@color/blue1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="96dp"
        android:layout_height="57dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="5dp"
        android:layout_marginTop="25dp"
        android:contentDescription="@string/todo"
        app:srcCompat="@drawable/read_book"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="340dp" />

    <TextView
        android:layout_width="297dp"
        android:layout_height="57dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="80dp"
        android:layout_marginTop="25dp"
        android:layout_marginEnd="22dp"
        android:background="@color/mlrs_lightBlue"
        android:fontFamily="sans-serif-condensed"
        android:lineSpacingExtra="0sp"
        android:paddingTop="10dp"
        android:text="@string/make_reservation"
        android:textAlignment="center"
        android:textColor="@color/mlrs_white_button"
        android:textSize="30sp"
        android:textStyle="bold"
        app:cornerRadius="?android:attr/buttonCornerRadius" />


</RelativeLayout>
